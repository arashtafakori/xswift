<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageOutputPath>C:\NuGetPackages\artaware</PackageOutputPath>
    <FileVersion></FileVersion>
    <AssemblyVersion></AssemblyVersion>
    <Version>1.1.0.6</Version>
    <IncludeSymbols>False</IncludeSymbols>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CoreX.Base" Version="1.0.8" />
    <PackageReference Include="CoreX.Domain" Version="1.0.8" />
    <PackageReference Include="CoreX.Settings" Version="1.1.0.2" />
    <PackageReference Include="EntityFrameworkCore.CoreX" Version="1.0.8" />
    <PackageReference Include="MassTransit.CoreX" Version="1.1.0.2" />
    <PackageReference Include="MongoDB.CoreX" Version="1.0.8" />
  </ItemGroup>

</Project>
